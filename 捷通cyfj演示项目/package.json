{
  "name": "vue2-vite-admin-template",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --mode development",
    "build": "run-s type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit --composite false",
    "lint:es": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:style": "stylelint src/**/*.{html,vue,css,sass,scss} --fix --quiet-deprecation-warnings",
    "lint": "run-p lint:es lint:style",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.6.1",
    "element-ui": "^2.15.14",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "vue": "^2.7.15",
    "vue-router": "^3.6.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@faker-js/faker": "^8.2.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/js-cookie": "^3.0.6",
    "@types/jsdom": "^21.1.3",
    "@types/lodash": "^4.14.201",
    "@types/node": "18",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vitejs/plugin-vue2-jsx": "^1.1.0",
    "@vue/eslint-config-standard-with-typescript": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.0.2",
    "npm-run-all2": "^6.1.1",
    "postcss-html": "^1.5.0",
    "sass": "~1.32.12",
    "stylelint": "^15.11.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "29",
    "stylelint-config-standard-scss": "^11.1.0",
    "typescript": "^5.2.2",
    "unplugin-icons": "^0.17.3",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.11",
    "vite-plugin-checker": "^0.6.2",
    "vue-tsc": "^1.8.22"
  }
}
