{
  "usingComponents": {
    "base-com": "/components/baseCom/baseCom"
  },
  "pages": [
    "pages/baseLearn/baseLearn",
    "pages/wxsLearn/wxsLearn",
    "pages/pullRefreshLoadLearn/pullRefreshLoadLearn",
    "pages/navigaterLearn/navigaterLearn",
    "pages/behaviorsLearn/behaviorsLearn",
    "pages/index/index",
    "pages/logs/logs",
    "custom-tab-bar/index"
  ],
  "subPackages": [
    {
      "root": "subPackageA",
      "name": "分包别名1",
      "pages": [
        "pages/cat/cat",
        "pages/dog/dog"
      ]
    },
    {
      "root": "subPackageB",
      "name": "分包别名2",
      "independent": true,
      "pages": [
        "pages/apple/apple",
        "pages/banana/banana"
      ]
    }
  ],
  "preloadRule": {
    "pages/navigaterLearn/navigaterLearn": {
      "packages": [
        "分包别名1"
      ],
      "network": "all"
    }
  },
  "window": {
    "navigationBarTitleText": "kzt",
    "navigationBarBackgroundColor": "#333",
    "navigationBarTextStyle": "white",
    "navigationStyle": "default",
    "backgroundColor": "#eee",
    "backgroundTextStyle": "dark"
  },
  "tabBar": {
    "color": "#333",
    "selectedColor": "#00ff00",
    "backgroundColor": "#efefef",
    "position": "bottom",
    "list": [
      {
        "pagePath": "pages/baseLearn/baseLearn",
        "text": "基础学习",
        "iconPath": "image/home.png",
        "selectedIconPath": "image/home-active.png"
      },
      {
        "pagePath": "pages/navigaterLearn/navigaterLearn",
        "text": "导航练习",
        "iconPath": "image/msg.png",
        "selectedIconPath": "image/msg-active.png"
      },
      {
        "pagePath": "pages/pullRefreshLoadLearn/pullRefreshLoadLearn",
        "text": "下拉刷新与上拉加载",
        "iconPath": "image/phone.png",
        "selectedIconPath": "image/phone-active.png"
      }
    ]
  },
  "style": "v2",
  "rendererOptions": {
    "skyline": {
      "defaultDisplayBlock": true,
      "disableABTest": true,
      "sdkVersionBegin": "3.0.0",
      "sdkVersionEnd": "15.255.255"
    }
  },
  "componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents"
}